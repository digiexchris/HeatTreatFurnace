{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch Test-app",
      "type": "cppdbg",
      "request": "launch",
      "program": "/home/chris/repos/PIDKiln/firmware/test-app/build/firmware/test-app/test_app",
      "args": ["-v", "-n", "PushAndPopS"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description": "Set Disassembly Flavor to Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ]
    },
    {
      "type": "gdbtarget",
      "request": "attach",
      "name": "Eclipse CDT GDB Adapter"
    },
    {
      "type": "espidf",
      "name": "Launch",
      "request": "launch"
    }
  ]
}